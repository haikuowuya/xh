<?xml version="1.0" encoding="utf-8"?>
<!--取消按钮的状态-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
     <item android:state_checked="true"
           android:drawable="@mipmap/ic_shopping_cart_checked"/>
    <item android:state_checked="false"
          android:drawable="@mipmap/ic_shopping_cart_unchecked"/>
    <item android:drawable="@mipmap/ic_shopping_cart_unchecked"/>

</selector>