<?xml version="1.0" encoding="utf-8"?>
<!--我的医生-预约挂号按钮的状态-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#883BB874"/> <corners android:radius="4dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="#3BB874"/> <corners android:radius="4dp"/>
        </shape>
    </item>

</selector>